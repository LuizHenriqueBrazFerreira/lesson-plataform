    "mysql2": "^3.9.2",
    "nodemon": "^3.1.0",
    "sequelize": "^6.37.1",
    "sequelize-cli": "^6.6.2",
    "tsc": "^2.0.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.2",
    "@types/chai": "4.3.0",
    "@types/chai-http": "4.2.0",
    "@types/mocha": "9.1.0",
    "@types/node": "16.11",
    "@types/sinon": "10.0.11",
    "@types/sinon-chai": "3.2.9",
    "chai": "4.3.6",
    "chai-http": "4.3.0",
    "eslint": "6.8.0",
